<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions"/>
 <object name="portal_tabs" meta_type="CMF Action Category">
  <object name="index_html" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="who" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Who we are</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/who-we-are</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="what" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">What we do</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/what-we-do</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="where" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Where we work</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/where-we-work</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="themes" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Our themes</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/our-themes</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="publications" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Our publications</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/our-publications</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="user" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="intranet" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Intranet</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/intranet</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:member is not None</property>
   <property name="permissions">
   </property>
   <property name="visible">True</property>
  </object>
  <object name="intranet-login" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Staff Intranet</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/intranet</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:member is None and getattr(context, "portal_type", "") in ['Plone Site', 'HomePage']</property>
   <property name="permissions">
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
